module.exports = mongoose => {
	const Part = mongoose.model(
		"part",
		mongoose.Schema(
			{
				partNumber: Number,
				supplier: String, 
				supplierPartName: String,
				givenPartName: String,
				currentQuantity: Number,
				minimumQuantityRequired: Number,
				maximumQuantity: Number 
 				
			}
		)
	);

	return Part;
}