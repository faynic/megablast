import { Injectable } from '@angular/core';
import { Observable, of } from 'rxjs';


import { Part } from './part';
import { PARTS } from './parts';

@Injectable({
  providedIn: 'root'
})
export class PartsService {


  constructor() {}



  getParts(): Observable<Part[]> {
    const parts = of(PARTS);
    return parts;
  }


}
